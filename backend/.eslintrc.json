{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["warn", { "endOfLine": "auto" }],
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "off",
    "no-underscore-dangle": ["error", { "allow": ["_db"] }],
    "consistent-return": "off",
    "arrow-body-style": "off"
  }
}
